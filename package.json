{
  "name": "robert-hood-web",
  "version": "1.0.0",
  "description": "Robert Hood web app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kahlil/cycle-browserify-boilerplate.git"
  },
  "scripts": {
    "build": "browserify -d -e src/app.js -t babelify -o public/javascripts/bundle.js",
    "watch": "watchify -d -e src/app.js -t babelify -o public/javascripts/bundle.js -v",
    "server": "babel-node ./server.js",
    "prod:server": "node build/server.js",
    "start": "npm-run-all build --parallel watch server",
    "postinstall": "bower install && babel -d build server.js"
  },
  "devDependencies": {
    "exorcist": "^0.4.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "tape": "^4.4.0",
    "uglify-js": "^2.6.1",
    "vulcanize": "^1.14.7",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "@cycle/core": "^6.0.2",
    "@cycle/dom": "^9.0.2",
    "@cycle/http": "^8.2.0",
    "@cycle/isolate": "^1.2.0",
    "@cycle/history": "^2.0.1",
    "accounting": "^0.4.1",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "chokidar-cli": "^1.2.0",
    "cyclic-router": "^1.0.0",
    "d3": "^3.5.16",
    "ejs": "^2.4.1",
    "eslint": "^2.1.0",
    "eslint-config-cycle": "^3.2.0",
    "eslint-plugin-cycle": "^1.0.2",
    "express": "^4.13.4",
    "history": "^2.0.1",
    "npm-run-all": "^1.5.1",
    "ramda": "^0.19.1",
    "request": "^2.69.0",
    "require-globify": "^1.3.0",
    "rx": "^4.0.7",
    "rx-combine-latest-obj": "^1.0.2",
    "switch-path": "^1.1.3",
    "uglifyify": "^3.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "sourceMap": true,
          "sourceMapRelative": "."
        }
      ],
      "require-globify"
    ]
  },
  "license": "MIT"
}
